{
  "manifest_version": 2,
  "name": "sum-ext",
  "version": "1.0",

  "description": "Summarizes any web page",

  "icons": {
    "48": "icons/Foxy-SF.svg"
  },
  "browser_action": {
    "default_icon": "icons/Foxy-SF.svg",
    "default_title": "summarize",
    "default_popup": "popup/summarize.html"
  },
  "permissions": [
    "activeTab",
    "*://scout-stage.herokuapp.com/*",
    "*://*.amazonaws.com/*"
  ]
}
